<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0", xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Standard Analytics IO RSS</title>
    <description>RSS feed for Standard Analytics IO weblog</description>
    <atom:link href="http://blog.standardanalytics.io/rss" rel="self" type="application/rss+xml"/>    
    <% getPosts().forEach(function(post,i){ %>    
    <item>
      <title><%= post.title %></title>
      <link>http://blog.standardanalytics.io/<%= post.url %></link>
      <description><![CDATA[<%- post.preview %>]]></description>
      <pubDate><%= post.date.toUTCString() %></pubDate>
      <guid>http://blog.standardanalytics.io/<%= post.url %></guid>
    </item>    
    <% }) %>    
  </channel>
</rss>
