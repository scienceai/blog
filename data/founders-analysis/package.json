          "@vocab": "http://standardanalytics.io/ontologies/stats.jsonld"
        },
        "@type": "Proportion",
        "estimate": 0.61905,
        "statTest": {
          "@type": "ChisqTest",
          "statistic": 0.7619,
          "df": 1,
          "pValue": 0.19137
        }
      }
    }
